{
  "version": 2,
  "name": "thestormprofessional",
  "alias": ["thestormprofessional.com", "www.thestormprofessional.com"],
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/styles.css",
      "dest": "/public/styles.css"
    },
    {
      "src": "/script.js",
      "dest": "/public/script.js"
    },
    {
      "src": "/admin.js",
      "dest": "/public/admin.js"
    },
    {
      "src": "/thank-you.html",
      "dest": "/public/thank-you.html"
    },
    {
      "src": "/images/(.*)",
      "dest": "/public/images/$1"
    },
    {
      "src": "/admin.html",
      "dest": "/public/admin.html"
    },
    {
      "src": "/storm-coverage.html",
      "dest": "/public/storm-coverage.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/admin",
      "dest": "/server.js"
    },
    {
      "src": "/",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "ADMIN_EMAIL": "Liamseamus1198@gmail.com"
  }
} 